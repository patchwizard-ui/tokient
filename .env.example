# Tokient Insight Stream - .env.example (with API key auth)
NODE_ENV=production
PORT=3000

# Choose ONE of the following DB configs
DATABASE_URL=postgres://USER:PASS@HOST:5432/tokient
# or
SQLITE_PATH=./data/tokient.db

# CORS - comma separated origins
ALLOWED_ORIGINS=https://tokient-insight-stream.lovable.app

# Auth
REQUIRE_API_KEY=true
JWT_SECRET=replace_me

# Logging
LOG_LEVEL=info